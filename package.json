{
    "name": "graph-tp",
    "version": "0.0.1",
    "description": "GraphTP",
    "private": true,
    "main": "dist/app.js",
    "scripts": {
        "tsc": "tsc -p tsconfig.json",
        "start": "tsc && node --max-http-header-size=50000 dist/app.js",
        "dev": "npm run tsc && node --max-http-header-size=50000 --inspect dist/app.js",
        "debug": "npm run tsc && node  --max-http-header-size=50000 --inspect-brk dist/app.js",
        "test-raw": "run-s test:lint test:pretty test:spec",
        "test:integration": "mocha --exit -r ts-node/register src/tests/integration.ts",
        "test:unit": "mocha -r ts-node/register src/tests/unit.ts --max-http-header-size=50000 --exit",
        "test:spec": "mocha -r ts-node/register src/**/*.spec.ts --max-http-header-size=50000",
        "test:lint": "eslint --max-warnings=0 ./src/ --ext .ts",
        "test:pretty": "prettier \"src/**/*.ts\" --list-different",
        "test:unit:single": "func(){ mocha -r ts-node/register src/$1/*.spec.ts; }; func",
        "fix": "run-s fix:lint fix:pretty",
        "fix:lint": "eslint ./src/ --ext .ts --fix",
        "fix:pretty": "prettier \"src/**/*.ts\" --write",
        "test": "nyc npm run test-raw",
        "cov:html": "nyc report --reporter=html && open-cli coverage/index.html",
        "cov:check": "nyc report && nyc check-coverage --lines 70 --functions 70 --branches 50"
    },
    "author": "Michiel Valee",
    "license": "Proprietary",
    "dependencies": {
        "@types/cors": "^2.8.7",
        "@types/cytoscape": "^3.14.7",
        "@types/ioredis": "^4.17.3",
        "@types/moment-timezone": "^0.5.13",
        "@types/randomstring": "^1.1.6",
        "@types/tesseract.js": "0.0.2",
        "@types/tough-cookie": "^4.0.0",
        "@types/uuid": "^8.0.1",
        "axios": "^0.19.2",
        "axios-cache-adapter": "^2.5.0",
        "axios-cookiejar-support": "^1.0.0",
        "cache-manager": "^3.3.0",
        "cache-manager-redis": "^0.6.0",
        "cors": "^2.8.5",
        "cytoscape": "^3.15.2",
        "cytoscape-cola": "^2.4.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "interval-promise": "^1.4.0",
        "ioredis": "^4.17.3",
        "js-sha256": "^0.9.0",
        "json-beautify": "^1.1.1",
        "md5": "^2.3.0",
        "mime-sniffer": "^0.0.3",
        "moment": "^2.27.0",
        "moment-timezone": "0.5.28",
        "object-hash": "^2.0.3",
        "os": "^0.1.1",
        "p-limit": "^3.0.2",
        "p-queue": "^6.6.0",
        "randomstring": "^1.1.5",
        "redis": "^3.0.2",
        "request-promise-native": "^1.0.9",
        "socket.io": "^2.2.0",
        "socket.io-client": "^2.2.0",
        "throttled-queue": "^1.0.7",
        "uuid": "^8.3.0",
        "winston": "^3.3.3",
        "winston-logzio": "^5.1.2",
        "xmlhttprequest": "^1.8.0"
    },
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/dockflow/docky-scraper.git"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@types/cache-manager": "^2.10.3",
        "@types/chai": "^4.2.12",
        "@types/cheerio": "^0.22.18",
        "@types/express": "^4.17.6",
        "@types/filesystem": "0.0.29",
        "@types/geojson": "^7946.0.7",
        "@types/mocha": "^8.0.1",
        "@types/object-hash": "^1.3.3",
        "@types/puppeteer": "^3.0.1",
        "@types/qs": "^6.9.4",
        "@types/redis": "^2.8.26",
        "@types/request": "^2.48.5",
        "@types/request-promise-native": "^1.0.17",
        "@types/socket.io": "^2.1.10",
        "@types/socket.io-client": "^1.4.33",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "assert": "^2.0.0",
        "chai": "^4.2.0",
        "copyfiles": "^2.3.0",
        "eslint": "^7.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "husky": "^4.2.5",
        "ioredis-mock": "^4.21.2",
        "mocha": "^8.1.3",
        "nodemon": "^2.0.4",
        "npm-run-all": "^4.1.5",
        "nyc": "^15.1.0",
        "open-cli": "^6.0.1",
        "prettier": "^2.1.1",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.7"
    },
    "nyc": {
        "extends": "@istanbuljs/nyc-config-typescript",
        "exclude": [
            "**/*.spec.js"
        ]
    },
    "husky": {
        "hooks": {}
    }
}
